1
1
---
list1 = foo bar foobar ;
list2 = a b ;
condition = ;
for element1 in $(list1) {
	Echo $(element1) ;
	for element2 in $(list2) {
		Echo 1 ;
		if $(condition) {
			continue ;
		}
		Echo $(element2) ;
	}
}
