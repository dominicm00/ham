paths =
	foo
	bar.a
	foobar.bee
	path/foo
	path/foo.a
	some/path/bar.bee
	/an/absolute/path/foobar.so
	/
;
for result in "$(paths:%1)" {
	Echo $(result) ;
}
---
G=
-
foo
bar.a
foobar.bee
path/foo
path/foo.a
some/path/bar.bee
/an/absolute/path/foobar.so
/
