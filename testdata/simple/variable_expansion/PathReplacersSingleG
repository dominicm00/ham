paths =
	foo
	bar.a
	foobar.bee
	path/foo
	path/foo.a
	some/path/bar.bee
	/an/absolute/path/foobar.so
	/
	some/path/bar.bee(archive.o)
	<oldgrist>foo
	<oldgrist>bar.a
	<oldgrist>foobar.bee
	<oldgrist>path/foo
	<oldgrist>path/foo.a
	<oldgrist>some/path/bar.bee
	<oldgrist>/an/absolute/path/foobar.so
	<oldgrist>/
	<oldgrist>some/path/bar.bee(archive.o)
;
for result in "$(paths:%1)" {
	Echo $(result) ;
}
