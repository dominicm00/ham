paths = %1 ;
for result in "$(paths:%2)" {
	Echo $(result) ;
}
---
#!{
	foo
	bar.a
	foobar.bee
	path/foo
	path/foo.a
	some/path/bar.bee
	/an/absolute/path/foobar.so
	/
	some/path/bar.bee(archive.o)
	<oldgrist>foo
	<oldgrist>bar.a
	<oldgrist>foobar.bee
	<oldgrist>path/foo
	<oldgrist>path/foo.a
	<oldgrist>some/path/bar.bee
	<oldgrist>/an/absolute/path/foobar.so
	<oldgrist>/
	<oldgrist>some/path/bar.bee(archive.o)
#!}
G=:R=
-
foo
bar.a
foobar.bee
path/foo
path/foo.a
some/path/bar.bee
/an/absolute/path/foobar.so
/
some/path/bar.bee(archive.o)
foo
bar.a
foobar.bee
path/foo
path/foo.a
some/path/bar.bee
/an/absolute/path/foobar.so
/
some/path/bar.bee(archive.o)
---
#!repeat
G=grist:R=
-
<grist>foo
<grist>bar.a
<grist>foobar.bee
<grist>path/foo
