G=grist:R=root
-
<grist>root/foo
<grist>root/bar.a
<grist>root/foobar.bee
<grist>root/path/foo
<grist>root/path/foo.a
<grist>root/some/path/bar.bee
<grist>/an/absolute/path/foobar.so
<grist>/
<grist>root/some/path/bar.bee(archive.o)
<grist>root/foo
<grist>root/bar.a
<grist>root/foobar.bee
<grist>root/path/foo
<grist>root/path/foo.a
<grist>root/some/path/bar.bee
